<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mints.shroom" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="NfcPlugin">
        <param name="android-package" value="com.chariotsolutions.nfc.plugin.NfcPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Immersify">
        <param name="android-package" value="com.toluhta.immersify.ImmersiveMode" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <name>Shroom</name>
    <description>
        Bar management application for Tims Mints.
    </description>
    <author email="matiss.strautmanis@gmail.com">
        tims mints dev team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
